[
    {
    "$id": "http://griffon.adobe.com/schemas/aep-mobile/trackState",
    "shortDesc": "Analytics Track State Event",
    "group": "event",
    "type": "object",
    "properties": {
        "uuid": {
            "alias": "rootId",
            "description": "Uniquely identifies each event",
            "type": "string"
        },
        "timestamp": {
            "description": "When the event occurred",
            "type": "number",
            "match": true
        },
        "annotations": {
            "description": "Array of Annotation objects",
            "type": "array",
            "items": {
                "shortDesc": "Annotation Object",
                "group": "construct",
                "type": "object",
                "properties": {
                    "payload": {
                        "description": "An object with custom data describing the annotation",
                        "type": "object"
                    },
                    "type": {
                        "alias": "namespace",
                        "description": "This is to scope annotations and prevent overwrites from other plugins. The type is usually determined by the plugin writing the annotation",
                        "type": "string"
                    },
                    "uuid": {
                        "description": "Uniquely identifies each annotation",
                        "type": "string"
                    }
                },
                "required": [
                    "type",
                    "uuid"
                ]
            }
        },
        "clientId": {
            "description": "A unique id that differentiates clients from one another",
            "type": "string"
        },
        "type": {
            "alias": "rootType",
            "description": "The type of event",
            "type": "string",
            "inherit": true,
            "const": "generic",
            "match": false
        },
        "vendor": {
            "description": "The vendor of the plugin that sent the event",
            "type": "string",
            "mock": "com.adobe.mobile.sdk"
        },
        "payload": {
            "inherit": true,
            "type": "object",
            "properties": {
                "ACPExtensionEventNumber": {
                    "alias": "sdkEventNumber",
                    "description": "The event number generated by the SDK",
                    "type": "integer"
                },
                "ACPExtensionEventUniqueIdentifier": {
                    "alias": "eventId",
                    "description": "The unique event id",
                    "type": "string"
                },
                "ACPExtensionEventData": {
                    "inherit": true,
                    "type": "object",
                    "properties": {
                        "state": {
                            "description": "The state that should be tracked",
                            "type": "string",
                            "match": true
                        },
                        "contextdata": {
                            "alias": "contextData",
                            "description": "Context data sent with the track call",
                            "type": "object"
                        }
                    },
                    "required": [
                        "contextdata",
                        "state"
                    ]
                },
                "ACPExtensionEventSource": {
                    "inherit": true,
                    "const": "com.adobe.eventsource.requestcontent"
                },
                "ACPExtensionEventType": {
                    "inherit": true,
                    "const": "com.adobe.eventtype.generic.track"
                }
            },
            "additionalProperties": false,
            "required": [
                "ACPExtensionEventSource",
                "ACPExtensionEventType",
                "ACPExtensionEventData"
            ]
        }
    },
    "required": [
        "clientId",
        "timestamp",
        "type",
        "uuid"
    ]
}
]